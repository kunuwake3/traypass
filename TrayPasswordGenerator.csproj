<PropertyGroup>
   <OutputType>WinExe</OutputType>
   <TargetFramework>net8.0-windows</TargetFramework>
   <UseWindowsForms>true</UseWindowsForms>

   <!-- однобиновый вывод оставляем -->
   <PublishSingleFile>true</PublishSingleFile>

-  <PublishTrimmed>true</PublishTrimmed>          <!-- УДАЛИТЬ -->
-  <PublishAot>true</PublishAot>                  <!-- УДАЛИТЬ (если был) -->
-  <WindowsFormsEnableTrim>true</WindowsFormsEnableTrim><!-- УДАЛИТЬ (если был) -->

   <RuntimeIdentifier>win-x64</RuntimeIdentifier>
   <ApplicationIcon>tray_favicon.ico</ApplicationIcon>
   <Nullable>enable</Nullable>
   <ImplicitUsings>true</ImplicitUsings>
</PropertyGroup>


